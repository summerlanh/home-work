<Window x:Class="HelloWorld.NavigatorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HelloWorld"
        mc:Ignorable="d"
        Title="NavigatorWindow" Height="300" Width="300">
   <Window.Background>
      <ImageBrush ImageSource="image\wpf-world.png" />
   </Window.Background>
   <StackPanel>
      <TextBlock Margin="10,10">Enter an address
         <Run Foreground="Red" FontWeight="Bold">Don't forget http://</Run>
      </TextBlock>
      <TextBox Name="uxTextBox"></TextBox>
      <Button Click="Button_Click">Go</Button>
   </StackPanel>
</Window>
